{
    "compilerOptions": {
        "target": "ES2015",
        "lib": ["dom", "es2017"], 
        "noImplicitAny": false,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "removeComments": false,
    }
}
